{
  "name": "typac",
  "version": "0.1.8",
  "description": "cli wrapper over npm and yarn for installing npm packages along with corresponding typings",
  "keywords": [
    "cli",
    "typescript",
    "typings",
    "install"
  ],
  "repository": "git@github.com:ewgenius/typac.git",
  "author": "Evgeniy Khramkov <ewgeniux@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf build && tsc -p ./",
    "prepublish": "npm run build"
  },
  "bin": {
    "typac": "./bin/typac.js"
  },
  "files": [
    "bin",
    "build",
    "LICENSE.md",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@npm/types": "^1.0.1",
    "@types/commander": "^2.3.31",
    "@types/node": "^6.0.117",
    "@types/node-fetch": "^2.1.2",
    "@types/npmlog": "^4.1.1",
    "tslint": "^4.5.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "npm-registry-fetch": "^3.8.0"
  }
}
